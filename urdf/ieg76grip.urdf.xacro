<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="ieg76">
<xacro:property name="pi" value="3.1415926535897931" />
	
  <link name="world"/>
  <joint name="mount" type="fixed" >
   <parent link="world" />
   <child link="base" />
	 <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
  </joint>
 
	<link name="base">
	 <visual>
	  <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
       <mesh filename="package://weiss_gripper_ieg76/meshes/ieg76.stl" />
      </geometry>
      <material name="Black">
       <color rgba="0.0 0.0 0.0 1.0"/>
      </material>      
     </visual>
	 <collision>
	  <geometry>
      <mesh filename="package://weiss_gripper_ieg76/meshes/ieg76.stl" />
	  </geometry>
	 </collision>
	</link>

	<joint name="jaw_position" type="prismatic" >
     <parent link="base" />
     <child link="finger_right" />
	 <origin xyz="0.0 -0.0255 0.088" rpy="0.0 0.0 ${pi}" />
	 <axis xyz="1 0 0" />
	 <limit lower="-0.015" upper="0.026" effort="100.0" velocity="100.0"/>
  </joint>

	<link name="finger_right">
	 <visual>
    <geometry>
      <xacro:insert_block name="mesh"/>
    </geometry>
    <material name="grey ">
     <color rgba="0.5 0.5 0.5 1.0"/>
    </material>      
   </visual>
	 <collision>
	  <geometry>
      <xacro:insert_block name="mesh"/>
	  </geometry>
	 </collision>
	</link>

	<joint name="jaw_position_mimic" type="prismatic" >
   <parent link="base" />
   <child link="finger_left" />
	 <origin xyz="0.0 0.0255 0.088" rpy="0.0 0.0 0.0" />
	 <axis xyz="1 0 0" />
	 <limit lower="-0.015" upper="0.026" effort="100.0" velocity="100.0"/>
	 <mimic joint="jaw_position" multiplier="1" />
        </joint>

	<link name="finger_left">
	 <visual>
    <geometry>
      <xacro:insert_block name="mesh"/>
    </geometry>
    <material name="grey ">
     <color rgba="0.5 0.5 0.5 1.0"/>
    </material>    
   </visual>
	 <collision>
	  <geometry>
      <xacro:insert_block name="mesh"/>
	  </geometry>
	 </collision>
	</link>

</robot>
